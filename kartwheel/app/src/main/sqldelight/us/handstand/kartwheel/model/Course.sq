CREATE TABLE course (
    id TEXT NOT NULL PRIMARY KEY,
    createdAt TEXT,
    deletedAt TEXT,
    distance REAL NOT NULL DEFAULT 0,
    maxRegistrants INTEGER DEFAULT 0,
    name TEXT,
    startLat REAL DEFAULT 0,
    startLong REAL DEFAULT 0,
    updatedAt TEXT
);

select_all:
SELECT *
FROM course;

insert_into:
INSERT INTO course(id, createdAt, deletedAt, distance, maxRegistrants, name, startLat, startLong, updatedAt)
VALUES (?, ?, ?, ?, ?, ?, ?, ?, ?);