<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ItemView">
        <attr name="src" format="reference" />
        <attr name="title" format="string" />
        <attr name="description" format="string" />
    </declare-styleable>
    <declare-styleable name="GameInfoPlayerView">
        <attr name="playerName" format="string" />
        <attr name="playerNumber" format="string" />
        <attr name="isUser" format="boolean" />
    </declare-styleable>
    <declare-styleable name="TopCourseTimeView">
        <attr name="rank" format="integer" />
    </declare-styleable>

    <declare-styleable name="AnchoredBottomSheetBehavior">
        <attr name="anchorPoint" format="dimension" />
    </declare-styleable>
</resources>